#include <stdio.h>

int fib(int n){
    
    if(n==0){
        return 0;
    }else if(n == 1){
        return 1;
        
    }else{
        
   int a =0 ,b = 1, c;
   for(int i =2;i<=n;i++){
       c = a+b;
       a=b;
       b=c;
   }
   
   return b;
    }
}
int main()
{
    
    
    int a;
    printf("Enter your Value: ");
    scanf("%d",&a);
    
  
    
    int q = fib(a);
    
    printf("fib = %d",q);

    return 0;
}
