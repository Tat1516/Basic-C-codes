#include <stdio.h>

int main()
{
    
    int N;
    printf("Enter your Number:");
    scanf("%d",&N);
    
    if(N==0){
        printf("This is zero");
        return 0;
    }
   int msb = N & (1<<(sizeof(int)*8)-1);
   
   if(msb){
       printf("negative");
   }
   else{
       printf("Positive");
   }
}
